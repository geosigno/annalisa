{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "commentaires",
  "info": {
    "name": "commentaire",
    "description": "Commentaires relatif aux cours et exercices."
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "texte": {
      "type": "text"
    },
    "cour": {
      "model": "cours",
      "via": "commentaires"
    },
    "exercice": {
      "model": "exercice",
      "via": "commentaires"
    },
    "user": {
      "via": "commentaires",
      "plugin": "users-permissions",
      "model": "user"
    },
    "parentID": {
      "model": "commentaire"
    }
  }
}
